@model IEnumerable<POSWeb.KoronaStore>

@{
    //Layout = null;
    Layout = "~/Views/Shared/_LayoutTable.cshtml";
}
<div id="main-wrapper">
    <div class="row">
        <div class="col-md-12">
            <div class="panel panel-white">
                <div class="panel-heading clearfix">
                    <h4 class="panel-title">Korona Stores</h4>
                </div>

                <div class="clearfix cl"></div>


                <div class="panel-body">
                    <div class="table-responsive">
                        <table id="report" class="display table" style="width: 100%; cellspacing: 0;">
                            <thead>
                                <tr>
                                    <th>
                                        @Html.DisplayNameFor(model => model.sno)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Korona_StoreId)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.storeName)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Address)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.City)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.State)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.ZipCode)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.phoneNumber)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.email)
                                    </th>
                                    @*<th></th>*@
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.sno)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Korona_StoreId)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.storeName)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Address)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.City)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.State)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ZipCode)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.phoneNumber)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.email)
                                        </td>
                                        @*<td>
                                            @Html.ActionLink("Edit", "Edit", new { id = item.Korona_StoreId }) |
                                            @Html.ActionLink("Details", "Details", new { id = item.Korona_StoreId }) |
                                            @Html.ActionLink("Delete", "Delete", new { id = item.Korona_StoreId })
                                        </td>*@
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>